	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(modulefsdir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(modulefsdir)" && rm -f $$files
